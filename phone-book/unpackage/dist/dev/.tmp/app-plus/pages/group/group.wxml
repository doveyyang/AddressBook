<view bind:__l="__l"><cu-custom bgColor="bg-gradual-blue" isBack="{{true}}" vue-slots="{{['backText','content']}}"><view slot="backText">返回</view><view slot="content">组分类</view></cu-custom><view class="content"><view class="{{['cu-list grid','col-'+gridCol,gridBorder?'':'no-border']}}"><block wx:for="{{iconList}}" wx:for-item="item" wx:for-index="index" wx:key="index"><block wx:if="{{index<gridCol*2}}"><view data-event-opts="{{[['tap',[['gotoUrl',['$0'],[[['iconList','',index,'page']]]]]]]}}" class="cu-item" bindtap="__e"><view class="{{['cuIcon-'+item.icon,'text-'+item.color]}}"><block wx:if="{{item.badge!=0}}"><view class="cu-tag badge"><block wx:if="{{item.badge!=1}}"><block>{{item.badge>99?'99+':item.badge}}</block></block></view></block></view><text>{{item.name}}</text></view></block></block></view><view class="margin-top"><dovey-nav navlist="{{navList}}"></dovey-nav></view><view class="cu-list menu-avatar margin-top"><block wx:for="{{list}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="{{['cu-item group-item',modalName=='move-box-'+index?'move-cur':'']}}" data-target="{{'move-box-'+index}}" data-event-opts="{{[['touchstart',[['ListTouchStart',['$event']]]],['touchmove',[['ListTouchMove',['$event']]]],['touchend',[['ListTouchEnd',['$event']]]],['tap',[['gotogroup',['$0'],[[['list','',index,'id']]]]]]]}}" bindtouchstart="__e" bindtouchmove="__e" bindtouchend="__e" bindtap="__e"><view class=" cu-avatar round sm"><text class="cuIcon-group_fill text-pink"></text></view><view class="content"><view class="text-black">{{item.name}}</view></view><view class="action"></view><view class="move"><view data-event-opts="{{[['tap',[['delgroup',['$0'],[[['list','',index,'id']]]]]]]}}" class="bg-red" catchtap="__e">删除</view></view></view></block></view><view class="cu-list menu-avatar"><block wx:for="{{personList}}" wx:for-item="person" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['showDetail',['$0'],[[['personList','',index]]]]]]]}}" class="cu-item" bindtap="__e"><view class="cu-avatar round " style="{{('background-image:url(//addressbook.jingru88.com/uploads/images/'+person.head_img+');')}}"></view><view class="content"><view class="text-black">{{person.nickname}}</view><view class="text-gray text-sm flex"><text class="text-cut">{{''+person.company+''}}</text></view></view><view class="action"><view class="text-grey text-xs text-cut">{{person.position}}</view></view></view></block><block wx:if="{{personList.length==0}}"><view class="empty">未添加好友</view></block></view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="{{['cu-modal',modalName=='RadioModal'?'show':'']}}" bindtap="__e"><view data-event-opts="{{[['tap',[['',['$event']]]]]}}" class="cu-dialog btn-dlg" catchtap="__e"><button type="primary" data-event-opts="{{[['tap',[['editgroup',['$event']]]]]}}" bindtap="__e">编辑分组</button><button type="warn" data-target="DialogModal1" data-event-opts="{{[['tap',[['deletegroup',['$event']]]]]}}" bindtap="__e">删除分组</button></view></view><view class="{{['cu-modal',modalName=='DialogModal1'?'show':'']}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">Modal标题</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" bindtap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">Modal 内容。</view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-green text-green" bindtap="__e">取消</button><button data-event-opts="{{[['tap',[['delGroup',['$event']]]]]}}" class="cu-btn bg-green margin-left" bindtap="__e">确定</button></view></view></view></view></view></view>