<view bind:__l="__l"><view class="btn-row"><text>{{"分享组（"+name+"）给以下好友"}}</text><button type="primary" eplain data-event-opts="{{[['tap',[['ShareTo',['$event']]]]]}}" bindtap="__e">分享给他们</button></view><checkbox-group data-event-opts="{{[['change',[['CheckboxChange',['$event']]]]]}}" class="block" bindchange="__e"><block wx:for="{{checkbox}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="cu-form-group margin-top"><view class="title">{{item.nickname}}</view><checkbox class="{{[item.checked?'checked':'']}}" checked="{{item.checked?true:false}}" value="{{item.id+''}}"></checkbox></view></block></checkbox-group></view>